    <tr>
        <td>1</td>
        <td>2</td>
    </tr>

    <tr>
        <td>3</td>
        <td>4</td>
    </tr>

    <tr>
        <td>a</td>
        <td>b</td>
    </tr>
</table>
<br>
<table border="1" width="200px">
    <tr bgcolor="pink">
        <td valign="top">1</td>
        <td align="left">
            2 <br>
            2 <br>
            2 <br>
        </td>
    </tr>

    <tr>
        <td valign="middle">3</td>
        <td align="center">
            2 <br>
            2 <br>
            2 <br>
        </td>
    </tr>

    <tr>
        <td valign="bottom">a</td>
        <td align="right">
            2 <br>
            2 <br>
            2 <br>
        </td>
    </tr>

    <tr>
        <td colspan="2">1, 2</td>
    </tr>
</table>
